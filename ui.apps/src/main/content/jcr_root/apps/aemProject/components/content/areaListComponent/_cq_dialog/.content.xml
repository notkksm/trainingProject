<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Author Books"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        granite:class="cmp-author_editor"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <multifieldwithmap
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Multifield With Map"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <well
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/well"
                                fieldLabel="Book Details">
                                <items jcr:primaryType="nt:unstructured">
                                    <figures
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                        composite="{Boolean}true"
                                        fieldDescription="Books Details"
                                        fieldLabel="Book Details"
                                        required="{Boolean}false">
                                        <field
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container"
                                            emptyText="Books Details"
                                            name="./figures">
                                            <items jcr:primaryType="nt:unstructured">
                                                <list
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/select"
                                                    class="cq-dialog-dropdown-showhide-multi"
                                                    cq-dialog-dropdown-showhide-target=".figures-showhide-target"
                                                    fieldLabel="Figures"
                                                    name="type">
                                                    <datasource
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="/apps/dropDownLIsting"/>
                                                </list>
                                                <setSquare
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/container"
                                                    class="hide figures-showhide-target"
                                                    showhidetargetvalue="square">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <well
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/container">
                                                            <layout
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/foundation/layouts/well"/>
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <side
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldLabel="Side"
                                                                    name="./side"/>
                                                            </items>
                                                        </well>
                                                    </items>
                                                </setSquare>
                                                <setRectangle
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/container"
                                                    class="hide figures-showhide-target"
                                                    showhidetargetvalue="rectangle">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <well
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/container">
                                                            <layout
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/foundation/layouts/well"/>
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <firstside
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldLabel="First side"
                                                                    name="./firstside"/>
                                                                <Copy_x0020_of_x0020_firstside
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldLabel="Second side"
                                                                    name="./secondside"/>
                                                            </items>
                                                        </well>
                                                    </items>
                                                </setRectangle>
                                                <setCircle
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/container"
                                                    class="hide figures-showhide-target"
                                                    showhidetargetvalue="circle">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <well
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/container">
                                                            <layout
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/foundation/layouts/well"/>
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <radius
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldLabel="Radius"
                                                                    name="./radius"/>
                                                            </items>
                                                        </well>
                                                    </items>
                                                </setCircle>
                                            </items>
                                        </field>
                                    </figures>
                                </items>
                            </well>
                        </items>
                    </multifieldwithmap>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
